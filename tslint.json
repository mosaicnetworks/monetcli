{
	"extends": [
		"tslint:recommended",
		"tslint-config-prettier",
		"tslint-config-airbnb"
	],
	"linterOptions": {
		"exclude": ["node_modules/**/*.ts"]
	},
	"rules": {
		"no-console": false,
		"interface-name": false,
		"variable-name": false,
		"object-literal-sort-keys": false,
		"max-classes-per-file": false,
		"ban-types": false,
		"no-var-requires": false
	}
}
